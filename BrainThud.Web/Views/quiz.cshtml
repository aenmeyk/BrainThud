<section id="quiz-view" class="view">

    <div class="row-fluid">
        <button id="card" type="button" class="card span12 btn btn-warning" data-bind="text: currentCard.text, click: flipCard"></button>
    </div>

    <div class="row-fluid">
        <div class="span6">
            <button type="button" class="primary-button btn btn-danger" data-bind="click: setNextCard">Incorrect</button>
        </div>
        <div class="span6">
            <button type="button" class="primary-button btn btn-success" data-bind="click: setNextCard">Correct</button>
        </div>
    </div>

    @*    <span data-bind="text: ko.toJSON(currentCard)"></span>*@
</section>
